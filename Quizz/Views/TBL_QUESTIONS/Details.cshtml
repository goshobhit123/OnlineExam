@model IEnumerable<Quizz.Models.USER_REPORT>

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_LayoutA.cshtml";
}

<h2>Details</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.USER_ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.USER_NAME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.USER_SCORE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MAX_MARKS)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.USER_PAPER)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.USER_ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.USER_NAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.USER_SCORE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MAX_MARKS)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.USER_PAPER)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.TEST_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.TEST_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.TEST_ID })
        </td>
    </tr>
}

</table>
